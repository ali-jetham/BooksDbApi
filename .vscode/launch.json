{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "netcoredbg (launch)",
			"type": "coreclr",
			"request": "launch",
			"program": "${command:dotrush.activeTargetPath}",
			"preLaunchTask": "dotrush: Build",
			"env": {
					"ASPNETCORE_ENVIRONMENT": "Development",
					"ASTNETCORE_URLS": "https://localhost7202"

			}
		},
		{
			"name": "netcoredbg (attach)",
			"type": "coreclr",
			"request": "attach",
			"processId": "${command:dotrush.pickProcess}"
		},
		{
			"name": "LifeDbApi Debug",
			"type": "dotnet",
			"request": "launch",
			"projectPath": "${workspaceFolder}/LifeDbApi.csproj"
		},
	]
}